%%%Version prof
\newcommandx*{\prof}[2][2=phantom]	{
					\ifthenelse{\equal{\theversionProf}{1}}
						{#1}
						{\ifthenelse{\equal{#2}{phantom}}
								{\phantom{#1}\hspace{-0.2cm}}
								{#2}
						}
				}


